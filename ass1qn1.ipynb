   "source": [
    "def get_odd_numbers_from_input():\n",
    "    input_str = input(\"Enter a list of integers separated by spaces: \")\n",
    "    arr = [int(num) for num in input_str.split()]\n",
    "    return [num for num in arr if num % 2 != 0]\n",
    "\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "id": "975a3fae",
   "metadata": {},
