    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Enter a list of integers separated by spaces: 1 2 3 4 5 6 4 3 \n",
      "[1, 3, 5, 3]\n"
     ]
    }
   ],
   "source": [
    "odd_numbers = get_odd_numbers_from_input()\n",
    "print(odd_numbers)\n"
